{
    "name": "drparham/soapclient",
    "description": "Soap Client Wrapper",
    "keywords": [
        "pta",
        "skeleton"
    ],
    "homepage": "https://github.com/drparham/soapclient",
    "license": "MIT",
    "authors": [
        {
            "name": "Dustin Parham",
            "email": "dr.parham@gmail.com",
            "homepage": "",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Drparham\\SoapClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drparham\\SoapClient\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
